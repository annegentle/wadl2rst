                        <programlisting language="javascript">{
    "os-reset_status": {
        "status": "available",
        "attach_status": "detached",
        "migration_status": "migrating"
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                  <param name="status"
                         style="plain"
                         type="xsd:string"
                         required="false"
