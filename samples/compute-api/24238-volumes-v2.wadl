         <method name="POST" rax:id="extendVolume">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Extend volume size">
               <para role="shortdesc">
                Extends the size of a volume to a requested size, in gibibytes
                (GiB). Specify the <code>os-extend</code> action in the
                request body.
            </para>
               <para>Preconditions</para>
               <itemizedlist>
                  <listitem>
                     <para>Volume status must be <code>available</code>.</para>
                  </listitem>
               </itemizedlist>
               <itemizedlist>
                  <listitem>
                     <para>Sufficient amount of storage must exist to extend the volume.</para>
                  </listitem>
               </itemizedlist>
               <itemizedlist>
                  <listitem>
                     <para>The user quota must have sufficient volume storage.</para>
                  </listitem>
               </itemizedlist>
               <para>Troubleshooting</para>
               <itemizedlist>
                  <listitem>
                    <para>
                        An <code>error_extending</code> volume status
                        indicates that the request failed. Ensure that you
                        meet the preconditions and retry the request. If the
                        request fails again, investigate the storage back end.
                    </para>
                  </listitem>
               </itemizedlist>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "os-extend": {
        "new_size": 3
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                  <param name="new_size"
                         style="plain"
                         type="xsd:int"
                         required="true"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
                            The new size of the volume, in gibibytes (GiB).
                        </para>
                    </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="202"/>
         </method>
         <method name="POST" rax:id="resetVolume">
