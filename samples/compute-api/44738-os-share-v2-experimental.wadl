      <resource id="action" path="/v2/{tenant_id}/shares/{share_id}/action">
         <param name="X-Openstack-Manila-Api-Version"
                style="header"
                type="xsd:string"
                rax:id="">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>
                  A valid Shared File Systems API micro-version. For example,
                  <code>"X-Openstack-Manila-Api-Version: 2.6"</code>. If you
                  omit this header, the default micro-version is 2.0.
               </para>
            </wadl:doc>
         </param>
         <param name="X-OpenStack-Manila-API-Experimental"
                style="header"
                type="xsd:bool"
                required="true"
                rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                        Indicates that this is an experimental Shared File
                        Systems API request. Set to <code>true</code>. For
                        example, <code>"X-OpenStack-Manila-API-Experimental:
                        true"</code>.
                     </para>
                  </wadl:doc>
               </param>
         <param name="tenant_id" style="template" type="csapi:UUID" rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <para>
                        The tenant ID in a multi-tenancy cloud.</para>
                  </wadl:doc>
               </param>
         <param name="share_id" style="template" type="csapi:UUID" rax:id="">
                        <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                           <para>
                              The ID of the share.
                           </para>
                        </wadl:doc>
                     </param>
         <method name="POST" rax:id="migrateShare">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Migrate share">
               <para role="shortdesc">
            Migrates a share from one back end to another.
         </para>
               <para>
            You can migrate a share from one back end to another but both back
            ends must set the <code>driver_handles_share_servers</code>
            parameter to <code>False</code>. If a share driver handles one of
            the back ends, this action is not supported. You can configure a
            back end in the <code>manila.conf</code> file.
         </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <xsdxt:code>
                        <programlisting language="javascript">{
    "os-migrate_share": {
        "force_host_copy": false,
        "host": "manila2@ams_backend#AMS_BACKEND"
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
            
                  <param name="host"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The host pool of the destination back end, in this format:
      <code>host@backend#POOL</code>.
   </para>
                        <itemizedlist>
                           <listitem>
                              <para>
                                 <code>host</code>. The host name for the destination back end.</para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>backend</code>. The name of the destination back end.</para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>POOL</code>. The pool name for the destination back end.</para>
                           </listitem>
                        </itemizedlist>
                     </wadl:doc>
                  </param>
                  <param name="force_host_copy"
                         style="plain"
                         required="true"
                         type="xsd:bool"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      Enables or disables generic host-based forced migrations, which bypasses
      driver optimizations. Default value is <code>false</code>.
   </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="202"/>
         </method>
      </resource>
                        
