        <resource id="Images" path="/v1/images">
         <method name="GET" rax:id="listImages-v1">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List images">
               <para role="shortdesc">Lists all public VM images.</para>
            </wadl:doc>
            <request>
               <representation mediaType="application/xml">
                
                  <param name="name"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list by an image name,
            in string format.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="container_format"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list by a container format. A valid value is
            <code>aki</code>, <code>ami</code>, <code>ari</code>,
            <code>bare</code>, <code>docker</code>, <code>ova</code>, or
            <code>ovf</code>.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="disk_format"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list by a disk format. A valid value is
            <code>aki</code>, <code>ami</code>, <code>ari</code>,
            <code>iso</code>, <code>qcow2</code>, <code>raw</code>,
            <code>vhd</code>, <code>vdi</code>, or <code>vmdk</code>.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="status"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list by a status. A
            valid value is <code>queued</code>,
            <code>saving</code>, <code>active</code>,
            <code>killed</code>, <code>deleted</code>, or
            <code>pending_delete</code>.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="size_min"
                         style="query"
                         required="false"
                         type="xsd:string"
                         default="ALL"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list by a minimum
            image size, in bytes.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="size_max"
                         style="query"
                         required="false"
                         type="xsd:string"
                         default="ALL"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list by a maximum
            image size, in bytes.
        </para>
                     </wadl:doc>
                  </param>
                  <param name="changes-since"
                         style="query"
                         required="false"
                         type="xsd:dateTime"
                         default="ALL"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Filters the image list to those images that
            have changed since a time stamp
            value.
        </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
