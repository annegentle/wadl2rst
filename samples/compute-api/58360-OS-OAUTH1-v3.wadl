         <method name="POST" rax:id="createConsumer">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Create consumer">
               <para role="shortdesc">Enables a user to create a
                consumer.</para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample>
                        <xsdxt:code>
                           <programlisting language="javascript">{
    "consumer": {
        "description": "My consumer"
    }
}
</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
               </representation>
            </request>
            <response status="201">
               <param name="secret"
                      style="plain"
                      type="xsd:string"
                      required="true"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <para>A secret for the consumer, which is returned
                        only when the consumer is created.</para>
                  </wadl:doc>
               </param>
               <param name="description"
                      style="plain"
                      type="xsd:string"
                      required="false"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <para>The consumer description.</para>
                  </wadl:doc>
               </param>
               <param name="id"
                      style="plain"
                      type="csapi:UUID"
                      required="true"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <para>The UUID of the consumer. In OpenStack, the UUID
                        is the key.</para>
                  </wadl:doc>
               </param>
               <param name="links"
                      style="plain"
                      type="xsd:dict"
                      required="true"
                      rax:id="">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <para>Consumer links.</para>
                  </wadl:doc>
               </param>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook">
                    <xsdxt:sample>
                        <xsdxt:code>
                           <programlisting language="javascript">{
    "consumer": {
        "secret": "secretsecret",
        "description": "My consumer",
        "id": "7fea2d",
        "links": {
            "self": "http://identity:35357/v3/OS-OAUTH1/consumers/7fea2d"
        }
    }
}
</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
               </representation>
            </response> 

