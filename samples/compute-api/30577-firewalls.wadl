                        <resource path="/v2.0/fw/firewall_policies/{firewall_policy_id}/insert_rule"
                id="insert_rule">
         <param name="firewall_policy_id"
                style="template"
                type="csapi:UUID"
                rax:id="">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Firewall policy UUID">
               <para>
      The UUID of the firewall policy.
    </para>
            </wadl:doc>
         </param>
         <method name="PUT" rax:id="insert_ruleFirewallPolicy">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Insert rule into a firewall policy">
               <para role="shortdesc">
              Insert firewall rule into a policy.
            </para>
               <para>
              A firewall_rule_id is inserted relative to the position of the
              firewall_rule_id set in <code>insert_before</code> or
              <code>insert_after</code>. If <code>insert_before</code> is set,
              <code>insert_after</code> is ignored. If both
              <code>insert_before</code> and <code>insert_after</code> are not
              set, the new firewall_rule_id is inserted at the top of the
              policy.
            </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting xmlns="http://docbook.org/ns/docbook" language="javascript">{
    "firewall_rule_id": "7bc34b8c-8d3b-4ada-a9c8-1f4c11c65692",
    "insert_after": "a08ef905-0ff6-4784-8374-175fffe7dade",
    "insert_before": ""
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                
                  <param required="true"
                         name="firewall_rule_id"
                         style="plain"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The UUID of the firewall rule.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param required="false"
                         name="insert_before"
                         style="plain"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The UUID of the firewall_rule. A new firewall_rule
            will be inserted before this firewall_rule.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param required="false"
                         name="insert_after"
                         style="plain"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The UUID of the firewall_rule. A new firewall_rule
            will be inserted after this firewall_rule.
        </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting xmlns="http://docbook.org/ns/docbook" language="javascript">{
    "audited": false,
    "description": "",
    "firewall_list": [],
    "firewall_rules": [
        "a08ef905-0ff6-4784-8374-175fffe7dade",
        "7bc34b8c-8d3b-4ada-a9c8-1f4c11c65692",
        "8722e0e0-9cc9-4490-9660-8c9a5732fbb0"
    ],
    "id": "c69933c1-b472-44f9-8226-30dc4ffd454c",
    "name": "test-policy",
    "shared": false,
    "tenant_id": "45977fa2dbd7482098dd68d0d8970117"
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                
                  <param required="true"
                         name="audited"
                         style="plain"
                         type="xsd:boolean"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            Indicates whether the policy has been audited.
        </para>
                        <para>
            Each time that the firewall policy or its associated rules are
            changed, the API sets this attribute to <code>False</code>. To
            audit the policy, the policy owner must explicitly update the
            policy to set this attribute to <code>True</code>.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param required="true"
                         name="description"
                         style="plain"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
