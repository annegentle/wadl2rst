        <resource id="os-server-external-events"
                path="/v2.1/{tenant_id}/os-server-external-events">
         <param name="tenant_id" style="template" type="csapi:UUID" rax:id="">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>
      The UUID of the tenant in a multi-tenancy cloud.
   </para>
            </wadl:doc>
         </param>
         <method name="POST" rax:id="createEvents">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Run events">
               <para role="shortdesc">
                Creates one or more external events, which the API dispatches
                to the instance.
            </para>
               <para>
                You must assign this instance to a host. Otherwise, this call
                does not dispatch the event to the instance.
            </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "events": [
        {
            "name": "test-event",
            "tag": "foo",
            "status": "completed",
            "server_uuid": "3df201cf-2451-44f2-8d25-a4ca826fc1f3"
        }
    ]
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                  <param name="events"
                         style="plain"
                         type="xsd:dict"
                         required="true"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List events">
                        <para>
                            The action.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="name"
                         style="plain"
                         type="xsd:string"
                         required="true"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="name">
                        <para>
                            The event name. A valid value is
                            <code>network-changed</code>, <code>network-vif-plugged</code>,
                            <code>network-vif-unplugged</code>, or
                            <code>network-vif-deleted</code>.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="tag"
                         style="plain"
                         type="xsd:string"
                         required="false"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="tag">
                        <para>
                            A string value that identifies the event.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="server_uuid"
                         style="plain"
                         type="csapi:UUID"
                         required="true"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="server_uuid">
                        <para>
                            The UUID of the server instance to which the API
                            dispatches the event.
                        </para>
                        <para>
                            You must assign this instance to a host.
                            Otherwise, this call does not dispatch the event
                            to the instance.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="status"
                         style="plain"
                         type="xsd:string"
                         required="false"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="status">
                        <para>
                            The event status. A valid value is
                            <code>failed</code>, <code>completed</code>, or
                            <code>in-progress</code>. Default is
                            <code>completed</code>.
                        </para>
                    </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "events": [
