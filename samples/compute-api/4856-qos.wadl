                  </param>
                
                  <param required="false"
                         name="shared"
                         style="plain"
                         type="xsd:boolean"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      Set to <code>true</code> to share this policy with other tenants.
    </para>
                        <para>
      Default is <code>false</code>.
    </para>
                     </wadl:doc>
                  </param>
                
                  <param required="false"
