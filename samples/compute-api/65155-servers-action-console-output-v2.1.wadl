         <method name="POST" rax:id="showConsoleOutput">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Show console output (os-getConsoleOutput action)">
               <para role="shortdesc">
                Shows console output for a server instance.
            </para>
               <para>
                Specify the <code>os-getConsoleOutput</code> action in the
                request body.
            </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN"
                            title="Get console output: JSON request">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "os-getConsoleOutput": {
        "length": 50
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                
                  <param name="os-getConsoleOutput"
                         style="plain"
                         type="xsd:string"
                         required="true"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The action.
    </para>
                     </wadl:doc>
                  </param>
                  <param name="length" style="plain" rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Length">
                        <para>
                            The number of lines to fetch from the end of
                            console log. <code>-1</code> indicates unlimited.
                        </para>
                    </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            xml:lang="EN"
                            title="Get console output: JSON response">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "output": "FAKE CONSOLE OUTPUT\nANOTHER\nLAST LINE"
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
               </representation>
            </response>
         </method>
