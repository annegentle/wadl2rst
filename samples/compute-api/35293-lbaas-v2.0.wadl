            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="List load balancers">
               <para role="shortdesc">
                Lists all load balancers for the tenant account.
            </para>
               <para>
                The list might be empty. Each load balancer element in the
                list can contain these attributes:
            </para>
               <itemizedlist>
                  <listitem>
                    <para>
                        <code>id</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>tenant_id</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>name</code>
                     </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>description</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>vip_subnet_id</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>vip_address</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>admin_state_up</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>listeners</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>provisioning_status</code>
                    </para>
                  </listitem>
                  <listitem>
                    <para>
                        <code>operating_status</code>
                    </para>
                  </listitem>
               </itemizedlist>
            </wadl:doc>
