                  <param name="Content-Length"
                         style="header"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The length of the response body that contains the
            list of names. If the operation fails, this value is the
            length of the error text in the response body.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Container-Object-Count"
                         style="header"
                         required="true"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The number of objects.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="Accept-Ranges"
                         style="header"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The type of ranges that the object accepts.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Container-Meta-name"
                         style="header"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The custom container metadata item, where <code>{name}</code>
            is the name of the metadata item.
        </para>
                        <para>
            One <code>X-Container-Meta-{name}</code> response header appears
            for each metadata item (for each <code>{name}</code>).
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Timestamp"
                         style="header"
                         required="true"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The date and time in <link xlink:href="https://en.wikipedia.org/wiki/Unix_time">UNIX Epoch
            time stamp format</link> when the account, container, or object
            was initially created as a current version.
        </para>
                        <para>
            For example, <code>1440619048</code> is equivalent to <code>Mon,
            Wed, 26 Aug 2015 19:57:28 GMT</code>.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Container-Bytes-Used"
                         style="header"
                         required="true"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The total number of bytes used.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="Content-Type"
                         style="header"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The MIME type of the list of names. If the
            operation fails, this value is the MIME type of the error
            text in the response body.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Trans-Id"
                         style="header"
                         required="true"
                         type="csapi:uuid"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            A unique transaction ID for this request. Your service
            provider might need this value if you report a problem.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="Date"
                         style="header"
                         required="true"
                         type="xsd:dateTime"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The transaction date and time.
        </para>
                        <para>
            The date and time stamp format is <link xlink:href="https://en.wikipedia.org/wiki/ISO_8601">ISO 8601</link>:
        </para>
                        <programlisting>CCYY-MM-DDThh:mm:ss±hh:mm</programlisting>
                        <para>
            For example, <code>2015-08-27T09:49:58-05:00</code>.
        </para>
                        <para>
            The <code>±hh:mm</code> value, if included, is the time zone as an
            offset from UTC. In the previous example, the offset value is
            <code>-05:00</code>.
        </para>
                        <para>
            A <code>null</code> value indicates that the token never expires.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="name"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The name of the container.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="hash"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The MD5 checksum value of the object content.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="bytes"
                         style="plain"
                         required="true"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The total number of bytes that are stored in Object
            Storage for the account.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="content_type"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The content type of the object.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="last_modified"
                         style="plain"
                         required="true"
                         type="xsd:dateTime"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The date and time when the object was last modified.
        </para>
                        <para>
            The date and time stamp format is <link xlink:href="https://en.wikipedia.org/wiki/ISO_8601">ISO 8601</link>:
        </para>
                        <programlisting>CCYY-MM-DDThh:mm:ss±hh:mm</programlisting>
                        <para>
            For example, <code>2015-08-27T09:49:58-05:00</code>.
        </para>
                        <para>
            The <code>±hh:mm</code> value, if included, is the time zone as an
            offset from UTC. In the previous example, the offset value is
            <code>-05:00</code>.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Container-Meta-Temp-URL-Key"
                         style="header"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The secret key value for temporary URLs.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Container-Meta-Temp-URL-Key-2"
                         style="header"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            A second secret key value for temporary URLs. The second key
            enables you to rotate keys by having two active keys at the same
            time.
        </para>
