              base="https://neutron.example.com/"
              xml:id="os-service-type-v2">
      <resource id="service-providers" path="/v2.0/service-providers">
         <method name="GET" rax:id="listServiceProviders">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="List service providers">
               <para role="shortdesc">
          Lists service providers and their associated service types.
         </para>
            </wadl:doc>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "service_providers": [
        {
            "service_type": "LOADBALANCER",
            "default": true,
            "name": "haproxy"
        }
    ]
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
            
                  <param name="service_providers"
                         style="plain"
                         type="xsd:list"
                         required="true"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      A list of <code>service_provider</code> objects.
    </para>
                     </wadl:doc>
                  </param>
            
                  <param name="service_type"
                         style="plain"
                         type="xsd:string"
                         required="true"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The service type, which is <code>CORE</code>, <code>DUMMY</code>,
      <code>FIREWALL</code>, <code>FLAVORS</code>, <code>L3_ROUTER_NAT</code>,
      <code>LOADBALANCER</code>, <code>LOADBALANCERV2</code>,
      <code>METERING</code>, <code>QOS</code>, or <code>VPN</code>.
    </para>
                     </wadl:doc>
                  </param>
            
                  <param name="default"
                         style="plain"
                         type="xsd:boolean"
                         required="true"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      Defines whether the provider is the default for the service type. If
      this value is <code>true</code>, the provider is the default. If this
      value is <code>false</code>, the provider is not the default.
    </para>
                     </wadl:doc>
                  </param>
            
                  <param name="name"
                         style="plain"
                         type="xsd:string"
                         required="true"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      User-facing provider name.
    </para>
                     </wadl:doc>
                  </param>
               </representation>
            </response> 
            <response status="400">
               <representation mediaType="application/xml" element="csapi:badRequest"/>
