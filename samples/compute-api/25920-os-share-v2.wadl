                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the share.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="status"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share status. A valid value is:
   </para>
                        <itemizedlist>
                           <listitem>
                              <para>
                                 <code>creating</code>. The share is being created.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>deleting</code>. The share is being deleted.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>error</code>. An error occurred during share
                       creation.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>error_deleting</code>. An error occurred during share
                       deletion.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>available</code>. The share is ready to use.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>manage_starting</code>. Share manage started.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>manage_error</code>. Share manage failed.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>unmanage_starting</code>. Share unmanage started.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>unmanage_error</code>. Share cannot be unmanaged.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>unmanaged</code>. Share was unmanaged.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>extending</code>. The extend, or increase, share size
            request was issued successfully.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>extending_error</code>. Extend share failed.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>shrinking</code>. Share is being shrunk.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>shrinking_error</code>. Failed to update quota on share shrinking.
         </para>
                           </listitem>
                           <listitem>
                              <para>
                                 <code>shrinking_possible_data_loss_error</code>. Shrink share
            failed due to possible data loss.
         </para>
                           </listitem>
                        </itemizedlist>
                     </wadl:doc>
                  </param>
                  <param name="project_id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the project in which the share was created.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="share_proto"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The Shared File Systems protocol. A valid value is <code>NFS</code>,
      <code>CIFS</code>, <code>GlusterFS</code>, or <code>HDFS</code>.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="size"
                         style="plain"
                         required="true"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share size, in GBs.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="name"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share name.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="description"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share description.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="display_name"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         default="None"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share name.
   </para>
                        <para>
      The Shared File Systems API supports the use of both <code>name</code>
      and <code>display_name</code> attributes, which are inherited attributes
      from the Block Storage API.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="display_description"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share description.
   </para>
                        <para>
      The Shared File Systems API supports the use of both <code>description</code>
      and <code>display_description</code> parameters, which are inherited attributes
      from the Block Storage API.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_type"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.6.) The UUID of the share type.
   </para>
                        <para>
      In minor versions, this parameter is a share type name, as a string.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_type_name"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.6.) The share type name.
   </para>
                        <para>
      Minor versions support only the <code>share_type</code> parameter
      where the share type name is expected.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="volume_type"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The volume type. The use of the <code>volume_type</code> object is
      deprecated but supported. It is recommended that you use the
      <code>share_type</code> object when you create a share type. When you
      issue a create a share type request, you can submit a request body with
      either a <code>share_type</code> or <code>volume_type</code> object. No
      matter which object type you include in the request, the API creates
      both a <code>volume_type</code> object and a <code>share_type</code>
      object. Both objects have the same ID. When you issue a list share types
      request, the response shows both <code>share_types</code> and
      <code>volume_types</code> objects.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="snapshot_id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the snapshot from which to create the share.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="is_public"
                         style="plain"
                         required="false"
                         type="xsd:bool"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The level of visibility for the share. Defines whether other tenants can or
      cannot see the share.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="metadata"
                         style="plain"
                         required="false"
                         type="xsd:dict"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      One or more metadata key and value pairs as a dictionary of strings.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_network_id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the share network.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="availability_zone"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The availability zone.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="export_location"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The export location.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="export_locations"
                         style="plain"
                         required="true"
                         type="xsd:list"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      A list of export locations. For example, when a share server has more
      than one network interface, it can have multiple export locations.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="host"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share host name.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="task_state"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.5.) For the share migration, the migration task state.
   </para>
                        <para>
      A valid value is <code>null</code>, <code>migration_starting</code>,
      <code>migration_error</code>, <code>migration_success</code>,
      <code>migration_completing</code>, or <code>migrating</code>. The
      <code>task_state</code> is <code>null</code> unless the share is
      migrated from one back-end to another. For details,
      see <code>os-migrate_share</code> extension request.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="share_server_id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the share server.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="consistency_group_id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.4.) The UUID of the consistency group where the share was created.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="snapshot_support"
                         style="plain"
                         required="true"
                         type="xsd:bool"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.2.) An extra specification that filters back ends by
      whether they do or do not support share snapshots.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="source_cgsnapshot_member_id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.4.) If the share was created with
      <code>consistency_group_id</code> attribute, the snapshot member ID.
   </para>
                        <para>
      The corresponding consistency group must be created from a snapshot with
      the current share as a member.
   </para>
                        <para>
      Consistency groups and snapshots are the part of the Shared File Systems
      experimental API. For details, see <link xlink:type="simple"
                                 xlink:href="api-ref-share-v2.html#consistency-groups">
      Consistency groups</link> and <link xlink:type="simple"
                                 xlink:href="api-ref-share-v2.html#consistency-group-snapshots">Consistency
      group snapshots</link>.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="created_at"
                         style="plain"
                         required="true"
                         type="xsd:dateTime"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
         The date and time stamp when the share was created.
      </para>
                        <para>
         The date and time stamp format is <link xlink:href="https://en.wikipedia.org/wiki/ISO_8601">ISO 8601</link>:
      </para>
                        <programlisting>CCYY-MM-DDThh:mm:ss±hh:mm</programlisting>
                        <para>
         The <code>±hh:mm</code> value, if included, returns the time zone as an
         offset from UTC.
      </para>
                        <para>
         For example, <code>2015-08-27T09:49:58-05:00</code>.
      </para>
                     </wadl:doc>
                  </param>
               </representation>
            </response>
         </method>
         <method name="GET" rax:id="listShares">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="List shares">
               <para role="shortdesc">
        Lists all shares.
      </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
          
                  <param name="all_tenants"
                         style="query"
                         required="false"
                         type="xsd:bool"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Admin only). Defines whether to list shares for all tenants.
      Set to <code>1</code> to list shares for all tenants. Set to
      <code>0</code> to list shares only for the current tenant.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="name"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share name.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="status"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      Filters by a share status. A valid value is
      <code>creating</code>, <code>error</code>, <code>available</code>,
      <code>deleting</code>, <code>error_deleting</code>,
      <code>manage_starting</code>, <code>manage_error</code>,
      <code>unmanage_starting</code>, <code>unmanage_error</code>,
      <code>unmanaged</code>, <code>extending</code>,
      <code>extending_error</code>, <code>shrinking</code>,
      <code>shrinking_error</code>, or
      <code>shrinking_possible_data_loss_error</code>.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_server_id"
                         style="query"
                         required="false"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the share server.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="metadata"
                         style="query"
                         required="false"
                         type="xsd:dict"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      One or more metadata key-value pairs, as a dictionary of strings.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="extra_specs"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The extra specifications as a set of one or more key-value
      pairs. In each pair, the key is the name of the extra specification
      and the value is the share type that was used to create the share.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_type_id"
                         style="query"
                         required="false"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      (Since API v2.6.) The UUID of the share type.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="limit"
                         style="query"
                         required="false"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The maximum number of shares to return.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="offset"
                         style="query"
                         required="false"
                         type="xsd:int"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The offset to define start point of share listing.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="sort_key"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The key to sort a list of shares. A valid value is <code>id</code>,
       <code>status</code>, <code>size</code>, <code>host</code>,
       <code>share_proto</code>, <code>export_location</code>,
       <code>availability_zone</code>, <code>user_id</code>,
       <code>project_id</code>, <code>created_at</code>,
       <code>updated_at</code>, <code>display_name</code>,
       <code>name</code>, <code>share_type_id</code>, <code>share_type</code>,
       <code>share_network_id</code>, <code>share_network</code>,
       <code>snapshot_id</code>, or <code>snapshot</code>.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="sort_dir"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The direction to sort a list of shares. A valid value
       is <code>asc</code>, or <code>desc</code>.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="snapshot_id"
                         style="query"
                         required="false"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The UUID of the snapshot that was used to create the share.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="host"
                         style="query"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The share host name.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_network_id"
                         style="query"
                         required="false"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The UUID of the share network.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="project_id"
                         style="query"
                         required="false"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The UUID of the project in which the share was created.
       Useful with <code>all_tenants</code> parameter.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="is_public"
                         style="query"
                         required="false"
                         type="xsd:bool"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       The level of visibility for the share. Set to <code>true</code>
       to list only public shares. Set to <code>false</code> to list only
       private shares.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="consistency_group_id"
                         style="query"
                         required="false"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
       (Since API v2.4.) The UUID of the consistency group where the share was
       created.
   </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                     <xsdxt:code>
                        <programlisting language="javascript">{
    "shares": [
        {
            "id": "d94a8548-2079-4be0-b21c-0a887acd31ca",
            "links": [
                {
                    "href": "http://172.18.198.54:8786/v1/16e1ab15c35a457e9c2b2aa189f544e1/shares/d94a8548-2079-4be0-b21c-0a887acd31ca",
                    "rel": "self"
                },
                {
                    "href": "http://172.18.198.54:8786/16e1ab15c35a457e9c2b2aa189f544e1/shares/d94a8548-2079-4be0-b21c-0a887acd31ca",
                    "rel": "bookmark"
                }
            ],
            "name": "My_share"
        },
        {
            "id": "406ea93b-32e9-4907-a117-148b3945749f",
            "links": [
                {
                    "href": "http://172.18.198.54:8786/v1/16e1ab15c35a457e9c2b2aa189f544e1/shares/406ea93b-32e9-4907-a117-148b3945749f",
                    "rel": "self"
                },
                {
                    "href": "http://172.18.198.54:8786/16e1ab15c35a457e9c2b2aa189f544e1/shares/406ea93b-32e9-4907-a117-148b3945749f",
                    "rel": "bookmark"
                }
            ],
            "name": "Share1"
        }
    ]
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
          
                  <param name="id"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The UUID of the share.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="name"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
