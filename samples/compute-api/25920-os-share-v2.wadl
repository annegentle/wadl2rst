                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share name.
   </para>
                     </wadl:doc>
                  </param>

                  <param name="description"
                         style="plain"
                         required="false"
                         type="xsd:string"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
      The share description.
   </para>
                     </wadl:doc>
                  </param>
                  <param name="share_type"
                         style="plain"
                         required="true"
                         type="csapi:UUID"
                         rax:id="">
