                    <resource id="snapshot_id"
                path="/v2.1/{tenant_id}/os-assisted-volume-snapshots/{snapshot_id}">
         <param name="tenant_id" style="template" type="csapi:UUID" rax:id="">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>
      The UUID of the tenant in a multi-tenancy cloud.
   </para>
            </wadl:doc>
         </param>
         <param name="snapshot_id" style="template" type="csapi:UUID" rax:id="">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>
         The UUID of the snapshot.
      </para>
            </wadl:doc>
         </param>
         <method name="DELETE" rax:id="deletesnapshots">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Delete assisted volume snapshot">
               <para role="shortdesc">
                Deletes an assisted volume snapshot.
            </para>
               <para>
                To make this request, add the <code>delete_info</code>
                query parameter to the URI, as follows:
            </para>
               <programlisting language="json">DELETE /os-assisted-volume-snapshots?delete_info='{"volume_id": "521752a6-acf6-4b2d-bc7a-119f9148cd8c"}'</programlisting>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <param name="delete_info"
                         style="query"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
                            Information for snapshot deletion. Include the ID
                            of the associated volume. For example:
                        </para>
                        <programlisting language="json">DELETE /os-assisted-volume-snapshots?delete_info='{"volume_id": "521752a6-acf6-4b2d-bc7a-119f9148cd8c"}'</programlisting>
                    </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="204">
        </response>
         </method>
      </resource>
                        

                        
                    
                
