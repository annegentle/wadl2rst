        <resource id="os-assisted-volume-snapshots"
                path="/v2.1/{tenant_id}/os-assisted-volume-snapshots">
         <param name="tenant_id" style="template" type="csapi:UUID" rax:id="">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
               <para>
      The UUID of the tenant in a multi-tenancy cloud.
   </para>
            </wadl:doc>
         </param>
         <method name="POST" rax:id="createsnapshots">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Create assisted volume snapshots">
               <para role="shortdesc">Creates an assisted volume snapshot.</para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "snapshot": {
        "volume_id": "521752a6-acf6-4b2d-bc7a-119f9148cd8c",
        "create_info": {
            "snapshot_id": "421752a6-acf6-4b2d-bc7a-119f9148cd8c",
            "type": "qcow2",
            "new_file": "new_file_name"
        }
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                  <param name="volume_id"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The source volume ID.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="create_info"
                         style="plain"
                         required="true"
                         type="xsd:dict"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>Information for snapshot creation.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="snapshot_id"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The snapshot ID.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="type"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The snapshot type.
                              A valid value is <code>qcow2</code>.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="new_file"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The name of the qcow2 file
                            that Block Storage creates,
                            which becomes the active image for the VM.
                        </para>
                    </wadl:doc>
                  </param>
               </representation>
            </request>
            <response status="200">
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:code xmlns:db="http://docbook.org/ns/docbook">
                        <programlisting language="javascript">{
    "snapshot": {
        "id": 100,
        "volumeId": "521752a6-acf6-4b2d-bc7a-119f9148cd8c"
    }
}
</programlisting>
                     </xsdxt:code>
                  </wadl:doc>
                  <param name="id"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The snapshot ID.
                        </para>
                    </wadl:doc>
                  </param>
                  <param name="volume_id"
                         style="plain"
                         required="true"
                         type="xsd:string"
                         rax:id="">
                    <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>The source volume ID.
                        </para>
                    </wadl:doc>
                  </param>
               </representation>
            </response>
         </method>
