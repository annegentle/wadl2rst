         <method name="HEAD" rax:id="checkToken">
            <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN" title="Check token">
               <para role="shortdesc">
                Validates a token.
            </para>
               <para>
                This call is similar to <code>GET /auth/tokens</code> but no
                response body is provided even in the <code>X-Subject-Token</code>
                header.
            </para>
               <important>
                  <para>
                    The Identity API returns the same response as when the
                    subject token was issued by <code>POST /auth/tokens</code>
                    even if an error occurs because the token is not valid. An
                    HTTP <code>204</code> response code indicates that the
                    <code>X-Subject-Token</code> is valid.
                </para>
               </important>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                    <xsdxt:sample>
                        <xsdxt:code>
                           <programlisting language="javascript">Headers:
X-Auth-Token: 1dd7e3
X-Subject-Token: c67580
</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
                
                  <param name="X-Auth-Token"
                         style="header"
                         type="xsd:string"
                         required="true"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            A valid authentication token for an administrative user.
        </para>
                     </wadl:doc>
                  </param>
                
                  <param name="X-Subject-Token"
                         style="header"
                         type="xsd:string"
                         required="false"
                         rax:id="">
                     <wadl:doc xmlns="http://docbook.org/ns/docbook" xml:lang="EN">
                        <para>
            The authentication token for which you want to perform the
            operation.
        </para>
                     </wadl:doc>
                  </param>
               </representation>
            </request>
