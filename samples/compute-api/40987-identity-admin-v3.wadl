         <method name="POST" rax:id="authenticatePasswordExplicitUnscoped">
            <wadl:doc xmlns="http://docbook.org/ns/docbook"
                      xml:lang="EN"
                      title="Password authentication with explicit unscoped authorization">
               <para role="shortdesc">
                Authenticates an identity and generates a token. Uses the
                password authentication method with explicit unscoped
                authorization.
            </para>
               <para>
                The request body must include a payload that specifies the
                <code>password</code> authentication method, the credentials,
                and the <code>unscoped</code> authorization scope.
            </para>
            </wadl:doc>
            <request>
               <representation mediaType="application/json">
                  <wadl:doc xmlns="http://docbook.org/ns/docbook"
                            title="Password authentication explicit unscoped: JSON request">
                    <xsdxt:sample>
                        <xsdxt:code>
                           <programlisting language="javascript">{
    "auth": {
        "identity": {
            "methods": [
                "password"
            ],
            "password": {
                "user": {
                    "id": "ee4dfb6e5540447cb3741905149d9b6e",
                    "password": "devstacker"
                }
            }
        },
        "scope": "unscoped"
    }
}
</programlisting>
                        </xsdxt:code>
                    </xsdxt:sample>
                  </wadl:doc>
                
