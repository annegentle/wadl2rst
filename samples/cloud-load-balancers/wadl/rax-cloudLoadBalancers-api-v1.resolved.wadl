<!DOCTYPE application [
<!ENTITY % common SYSTEM "common.ent">
<!--
         A collection of common faults, these are pretty much expected
         in every request.
     --><!ENTITY ndash "&#8211;">
<!ENTITY commonFaults '
        <response status="400 500" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:loadbalancerFault"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="503" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:serviceUnavailable"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="400" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:badRequest"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="401" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:unauthorized"/>
            <representation mediaType="application/json"/>
        </response>
        
        
        <response status="413" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:overLimit"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!ENTITY lesscommonFaults '
        <response status="400 500" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:loadbalancerFault"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="503" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:serviceUnavailable"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="404" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:itemNotFound"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="401" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:unauthorized"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!--
          Faults on GET
      --><!ENTITY getFaults '
        <response status="404" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:itemNotFound"/>
            <representation mediaType="application/json"/>
        </response>'>
<!--
          Faults on POST/PUT
      --><!ENTITY postPutFaults '
        <response status="415" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:badMediaType"/>
            <representation mediaType="application/json"/>
        </response>
        <response status="400" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:NetworkNotFound"/>
            <representation mediaType="application/json"/>
        </response>'>
<!ENTITY rebootFaults '<response status="422" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:HTTPUnprocessableEntity"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!--
          Faults that can occur when we are building servers or images.
      --><!ENTITY buildFaults '
        <response status="503" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:serverCapacityUnavailable"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!--
          Holds build in progress which occurs when an operation fails
          because the server is in the process of being built.
      --><!ENTITY inProgressFault '
        <response status="409" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:buildInProgress"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!ENTITY badMethod '
        <response status="405" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:badMethod"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!ENTITY forbidden '
        <response status="403" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:forbidden"/>
            <representation mediaType="application/json"/>
        </response>
           '>
<!ENTITY immutableEntity '
        <response status="422" xmlns="http://wadl.dev.java.net/2009/02">
            <representation mediaType="application/xml" element="lbapi:immutableEntity"/>
            <representation mediaType="application/json"/>
        </response>
           '>
